{
  "manifest_version": 3,
  "name": "Tab Clusterer",
  "version": "1.0",
  "permissions": [
    "tabs",
    "bookmarks",
    "background"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
